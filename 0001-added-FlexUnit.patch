+			if(Text == null)
+				Text = "";
+			_tf = new TextField();
+			_tf.width = Width;
+			_tf.embedFonts = EmbeddedFont;
+			_tf.selectable = false;
+			_tf.sharpness = 100;
+			_tf.multiline = true;
