     ruby 'tools/*Test.rb'
 end
+
+task :testFlex do
+    sh "mxmlc -static-link-runtime-shared-libraries=true -sp=src,libs,test -library-path+=#{$flexunit} -o build/tests.swf test/TestRunner.mxml"
+end
